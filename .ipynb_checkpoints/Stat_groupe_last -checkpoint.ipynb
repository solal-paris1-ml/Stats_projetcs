   "source": [
    "# Weighted rating\n",
    "m = min(dta['Reviews'])\n",
    "C = dta['Rating'].mean()\n",
    "def weighted_rating(x, m=m, C=C):\n",
    "    v = x['Reviews']\n",
    "    R = x['Rating']\n",
    "    return (v/(v+m) * R) + (m/(m+v) * C)"
   ]
